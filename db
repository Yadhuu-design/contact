package pack;
import java.sql.*;
import com.mysql.jdbc.Connection;

public class Dbconnection {
	public Connection getConnection() {
		Connection con=null;
		try {
            Class.forName("com.mysql.cj.jdbc.Driver");
			con=(Connection) DriverManager.getConnection("jdbc:mysql://localhost:3306/java123","root","");
		}
		catch (Exception e){
			e.printStackTrace();
			
		}
		return con;
	}

}
